@model Project

<h3 class="bg-secondary text-white text-center p-1">Details</h3>
<table class="table table-sm table-bordered table-secondary text-black">
    <tbody>
        <tr><th>ID</th><td>@Model.ProjectId</td></tr>
        <tr><th>Name</th><td>@Model.ProjectName</td></tr>
        <tr><th>Status</th><td>@Model.Status</td></tr>
        <tr><th>Project Manager</th><td>@Model.ProjectManager</td></tr>
        <tr><th>Description</th><td>@Model.Description</td></tr>
        <tr><th>Created</th><td>@Model.ProjectCreated</td></tr>
        <tr><th>Updated</th><td>@Model.ProjectUpdated</td></tr>
    </tbody>
</table>
<a asp-controller="Project" asp-action="Projects" class="btn btn-dark float-left m-1">Back</a>

<a asp-controller="Project" asp-action="Delete" asp-route-id="@Model.ProjectId" onclick="return confirm('Are you sure you want to delete?');" class="btn btn-danger float-right m-1 ">Delete</a>
<a asp-controller="Project" asp-action="Edit" asp-route-id="@Model.ProjectId" class="btn btn-primary float-right m-1">Update</a>
<div style="height: 5vh"></div>

